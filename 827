#include <iostream>
#include <string>
#include <algorithm>

using namespace std;

int main() {
    string s;
    cin >> s;
    string s1 = s;
    reverse(s1.begin(), s1.end());

    if (s == s1) {
        cout << "yes";
    } else {
        cout << "no";
    }

    cin.get();
    cin.get();
    return 0;
}
