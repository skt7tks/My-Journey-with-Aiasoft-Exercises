#include <iostream>
using namespace std;

int main() {
    string s;
    cin >> s;
    int n = s.size(), c = 0;

    for (int i = 0; i < n; i++) {
        char x = s[i];
        if (x >= '0' && x <= '9') {
            c++;
        }
    }

    cout << c << endl;

    cin.get();
    cin.get();
    return 0;
}
