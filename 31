#include <iostream>
#include <algorithm>

using namespace std;

int main() {
    int n;
    cin >> n;
    int a[n];
    for (int i = 0; i < n; i++) {
        cin >> a[i];
    }
    swap(*min_element(a, a + n), a[0]);
    swap(*max_element(a, a + n), a[n - 1]);

    for (int i = 0; i < n; i++) {
        cout << a[i] << " ";
    }
cin.get();
cin.get();
return 0;
}
